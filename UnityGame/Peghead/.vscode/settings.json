{
    "workbench.colorCustomizations": {
        "editorCursor.background": "#ffffff",
        "editorCursor.foreground": "#ffffff",
        "editor.lineHighlightBackground": "#ff000000",
        "editor.lineHighlightBorder": "#ffffff78",
        "editor.selectionBackground": "#ffffff19",
        "activityBar.background": "#09090b",
        "editor.background": "#09090b",
        "sideBar.background": "#09090b",
        "sideBarSectionHeader.background": "#09090b",
        "sideBar.foreground": "#ffffff",
        "sideBarSectionHeader.border": "#ffffff",
        "sideBarSectionHeader.foreground": "#ffffff",
        "sideBarTitle.foreground": "#fff",
        "editor.wordHighlightBackground": "#00bfff67",
        "editor.wordHighlightBorder": "#ff000000",
        "editorBracketMatch.background": "#ffffff00",
        "editorBracketMatch.border": "#0f0",
        "panel.background": "#09090b",
        "panel.dropBorder": "#09090b",
        "panel.border": "#ff000000",
        "panelSectionHeader.background": "#09090b",
        "panelSection.border": "#ffffff",
        "panelSectionHeader.border": "#09090b",
        "panelSectionHeader.foreground": "#ffffff",
        "panelSection.dropBackground": "#ffffff",
        "terminal.background": "#09090b",
        "tab.activeBackground": "#09090b",
        "tab.inactiveBackground": "#09090b",
        "tab.activeForeground": "#fff",
        "tab.hoverBackground": "#000",
        "tab.hoverBorder": "#006fab",
        "tab.activeBorder": "#00a6ff",
        "statusBar.background": "#001921",
        "statusBar.border": "#fff",
        "statusBar.foreground": "#fff",
        "titleBar.activeBackground": "#09090b",
        "toolbar.activeBackground": "#006fab",
        "scrollbarSlider.activeBackground": "#006fab",
        "scrollbar.shadow": "#ff000000",
        "scrollbarSlider.background": "#001921",
        "scrollbarSlider.hoverBackground": "#006fab",
        "tab.border": "#09090b",
        "editorGroup.border": "#09090b",
        "editorGroupHeader.tabsBackground": "#09090b",
        "dropdown.border": "#fff",
        "menu.background": "#09090b",
        // "input.background": "#ff0000",
        "badge.background": "#00a6ff",
        "editor.foldBackground": "#ffffff11",
    },
    "editor.fontSize": 16,
    "editor.lineHeight": 20,
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "scope": [
                    "entity.name.function.cpp",
                    "entity.name.function.member.cpp",
                    "entity.name.function.call.cpp"
                ],
                "settings": {
                    "foreground": "#4fdbee",
                }
            },
            {
                "scope": [
                    "keyword",
                    "storage",
                    "keyword.control",
                    "keyword.other.unit.hexadecimal.cpp",
                    "keyword.operator.delete.cpp",
                    "keyword.operator.new.cpp",
                ],
                "settings": {
                    "foreground": "#ce7ff2"
                }
            },
            {
                "scope": [
                    "keyword.operator.comparison.cpp",
                    "keyword.operator.logical",
                    "keyword.operator.bitwise.shift.cpp",
                    "keyword.operator.assignment.compound",
                    "keyword.operator.bitwise.cpp",
                    "keyword.operator.arithmetic.cpp",
                    "keyword.operator.assignment.cpp",
                ],
                "settings": {
                    "foreground": "#009dff",
                }
            },
            {
                "scope": [
                    "entity.name.namespace",
                    "entity.name.type",
                    "variable.language.this.cpp",
                    "entity.name.type.class.templated.cpp",
                ],
                "settings": {
                    "foreground": "#ff9e61",
                    "fontStyle": "italic"
                }
            },
            {
                "scope": [
                    "variable",
                    "meta.function.definition.cpp"
                ],
                "settings": {
                    "foreground": "#c65d92",
                }
            },
            {
                "scope": [
                    "punctuation.terminator.statement.cpp",
                    "punctuation.separator.pointer-access.cpp",
                    "punctuation.separator.dot-access.cpp"
                ],
                "settings": {
                    "foreground": "#a1a1a1",
                }
            },
            {
                "scope": [
                    "constant.numeric.decimal",
                    "constant.numeric.hexadecimal.cpp",
                    "string",
                ],
                "settings": {
                    "foreground": "#1ca36d",
                    "fontStyle": "italic"
                }
            },
            {
                "scope": [
                    "string.quoted.other.lt-gt.include.cpp",
                    "string.quoted.double.include.cpp",
                ],
                "settings": {
                    "foreground": "#7ae7ff",
                }
            },
            {
                "scope": [
                    "variable.parameter.cpp"
                ],
                "settings": {
                    "foreground": "#766ee4"
                }
            },
            {
                "scope": [
                    "comment",
                    "comment.line.double-slash.cpp",
                    "punctuation.definition.comment.cpp",
                    "comment.block.cpp",
                    "punctuation.definition.comment.begin.cpp",
                    "punctuation.definition.comment.end.cpp"
                ],
                "settings": {
                    "foreground": "#433f62",
                }
            },
            {
                "scope": [
                    "variable.other.enummember.cpp",
                    "meta.enum.definition.cpp",
                    "meta.body.enum.cpp",
                    "meta.block.enum.cpp",
                ],
                "settings": {
                    "foreground": "#FF0000",
                }
            }
        ]
    },
    "editor.fontFamily": "\"Fira Code Light\"",
    "editor.fontLigatures": false,
    "files.exclude": {
        "**/.DS_Store": true,
        "**/.git": true,
        "**/.vs": true,
        "**/.gitmodules": true,
        "**/.vsconfig": true,
        "**/*.booproj": true,
        "**/*.pidb": true,
        "**/*.suo": true,
        "**/*.user": true,
        "**/*.userprefs": true,
        "**/*.unityproj": true,
        "**/*.dll": true,
        "**/*.exe": true,
        "**/*.pdf": true,
        "**/*.mid": true,
        "**/*.midi": true,
        "**/*.wav": true,
        "**/*.gif": true,
        "**/*.ico": true,
        "**/*.jpg": true,
        "**/*.jpeg": true,
        "**/*.png": true,
        "**/*.psd": true,
        "**/*.tga": true,
        "**/*.tif": true,
        "**/*.tiff": true,
        "**/*.3ds": true,
        "**/*.3DS": true,
        "**/*.fbx": true,
        "**/*.FBX": true,
        "**/*.lxo": true,
        "**/*.LXO": true,
        "**/*.ma": true,
        "**/*.MA": true,
        "**/*.obj": true,
        "**/*.OBJ": true,
        "**/*.asset": true,
        "**/*.cubemap": true,
        "**/*.flare": true,
        "**/*.mat": true,
        "**/*.meta": true,
        "**/*.prefab": true,
        "**/*.unity": true,
        "build/": true,
        "Build/": true,
        "Library/": true,
        "library/": true,
        "obj/": true,
        "Obj/": true,
        "Logs/": true,
        "logs/": true,
        "ProjectSettings/": true,
        "UserSettings/": true,
        "temp/": true,
        "Temp/": true
    },
    "dotnet.defaultSolution": "Peghead.sln"
}